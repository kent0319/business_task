<h1>Listing business_alerts</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Title</th>
      <th>Detail</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @business_alerts.each do |business_alert| %>
      <tr>
        <td><%= business_alert.date %></td>
        <td><%= business_alert.title %></td>
        <td><%= business_alert.detail %></td>
        <td><%= link_to 'Show', business_alert %></td>
        <td><%= link_to 'Edit', edit_business_alert_path(business_alert) %></td>
        <td><%= link_to 'Destroy', business_alert, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Business alert', new_business_alert_path %>
